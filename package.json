        "nodeaffinity": "^1.1.0",
        "readline": "^1.3.0",
        "ts-node": "^10.9.1"
    },
    "devDependencies": {
        "@types/node": "^20.6.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.2.2"
