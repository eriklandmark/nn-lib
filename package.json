    "author": "Erik Landmark",
    "license": "ISC",
    "private": false,
    "gypfile": true,
    "dependencies": {
        "apollo-server": "^3.12.0",
        "cli-progress": "^3.12.0",
        "gpu.js": "^2.16.0",
        "jimp": "^0.22.8",
        "node-addon-api": "^7.0.0",
        "nodeaffinity": "^1.1.0",
        "readline": "^1.3.0"
    },
    "devDependencies": {
        "@types/node": "^20.4.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.1.6"
    }
}
